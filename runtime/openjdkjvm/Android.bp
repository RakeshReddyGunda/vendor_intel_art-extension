cc_defaults {
    name: "libopenjdkjvm_defaults1",
    defaults: ["art_defaults1"],
    host_supported: true,
    srcs: ["OpenjdkJvm.cc"],
    include_dirs: ["art/runtime"],
    shared_libs: [
        "libbase",
        "libnativehelper"
    ],
}

art_cc_library {
    name: "libopenjdkjvm1",
    defaults: ["libopenjdkjvm_defaults1"],
    shared_libs: ["libart"],
}

art_cc_library {
    name: "libopenjdkjvmd1",
    defaults: [
        "art_debug_defaults1",
        "libopenjdkjvm_defaults1",
    ],
    shared_libs: ["libartd"],
}
